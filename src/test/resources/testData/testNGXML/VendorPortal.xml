<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Vendor Portal Suite" parallel="tests" thread-count="4">
    <listeners>
        <listener class-name="listners.ExtentTestNGListener"/>
    </listeners>
    <test name="Vendor Portal Test - Sam">
        <parameter name="dataPath" value="VendorPortalData/sam.json"/>
        <classes>
            <class name="vendorPortalTestCases.VendorPortalTestCases"/>
        </classes>
    </test>
    <test name="Vendor Portal Test - John">
        <parameter name="dataPath" value="VendorPortalData/john.json"/>
        <classes>
            <class name="vendorPortalTestCases.VendorPortalTestCases"/>
        </classes>
    </test>
</suite>